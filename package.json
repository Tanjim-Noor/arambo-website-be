{
  "name": "arambo-website-be",
  "version": "1.0.0",
  "description": "Backend API for Arambo Website - Property listings with MongoDB integration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "seed": "ts-node src/database/seeds/property.seed.ts",
    "db:seed": "npm run seed",
    "db:reset": "ts-node src/database/scripts/reset-database.ts",
    "db:reset:seed": "ts-node src/database/scripts/reset-database.ts seed",
    "migrate:create": "migrate-mongo create",
    "migrate:up": "migrate-mongo up",
    "migrate:down": "migrate-mongo down",
    "migrate:status": "migrate-mongo status"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "mongodb",
    "mongoose",
    "property",
    "api"
  ],
  "author": "Arambo Team",
  "license": "MIT",
  "dependencies": {
    "@types/mongoose": "^5.11.96",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "dotenv-expand": "^12.0.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "i": "^0.3.7",
    "mongoose": "^8.18.1",
    "morgan": "^1.10.1",
    "redis": "^5.8.2",
    "tslib": "^2.8.1",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "jest": "^30.1.3",
    "migrate-mongo": "^12.1.3",
    "mongodb-memory-server": "^10.2.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
